{
  // The template which will be used for spawn only
  // Example: 
  // {
  //     "name": "default",
  //     "desc": "",
  //     "file": "default.nbt",
  //     "spawns": "default",
  //     "direction": "south"
  // }
  "spawn": null,
  
  "spawns": {
	"shiba": [
	  [ 2, 10, 2 ]
	]
  },
  
  // The list of templates being available. The first entry is the default template.
  "templates": [
    {
      "name": "shiba",
      "desc": "",
      "file": "shiba.nbt",
      "spawns": "shiba",
      "direction": "south"
    }
  ]
}
